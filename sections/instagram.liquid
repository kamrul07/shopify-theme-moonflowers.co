	<a class="instagram-feed__itemm" href="{{ section.settings.insta_url2 }}" target="_blank">
    <img  alt="" src="{{ section.settings.instaBox_img2 | img_url: '501x' }}">
	</a>
    <a class="instagram-feed__itemm" href="{{ section.settings.insta_url3 }}" target="_blank" >
        <img class="lazy__img js-img x y o-cover" alt="" src="{{ section.settings.instaBox_img3 | img_url: '501x' }}">
    </a>

          
<a class="instagram-feed__itemm" href="{{ section.settings.insta_url4 }}" target="_blank" >
<img alt="" src="{{ section.settings.instaBox_img4 | img_url: '501x' }}">
</a>
          
<a class="instagram-feed__itemm" href="{{ section.settings.insta_url5 }}" target="_blank" >
    <img  alt="" src="{{ section.settings.instaBox_img5 | img_url: '501x' }}">
</a>
</div>
</div>
{% schema %}
{
	"name": "Instagram feed",
