<div class="instagram_section_wrapper desktop_version">
  <a href="{{ section.settings.insta_url }}" target="_blank"><h2>{{ section.settings.insta_title }}</h2> </a>
  <div class="instagram_section_wrapper_inner">
  <div class="instagram-feed__col">
    <a class="instagram-feed__item" href="{{ section.settings.insta_url11 }}" target="_blank" >
		<img  alt="" src="{{ section.settings.instaBox_img1 | img_url: '501x' }}">
    </a>
	<a class="instagram-feed__item" href="{{ section.settings.insta_url2 }}" target="_blank">
    <img  alt="" src="{{ section.settings.instaBox_img2 | img_url: '501x' }}">
	</a>
    <a class="instagram-feed__item" href="{{ section.settings.insta_url3 }}" target="_blank" >
        <img class="lazy__img js-img x y o-cover" alt="" src="{{ section.settings.instaBox_img3 | img_url: '501x' }}">
    </a>
  </div>
<div class="instagram-feed__col">
          
<a class="instagram-feed__item" href="{{ section.settings.insta_url4 }}" target="_blank" >
<img alt="" src="{{ section.settings.instaBox_img4 | img_url: '501x' }}">
</a>
          
<a class="instagram-feed__item" href="{{ section.settings.insta_url5 }}" target="_blank" >
    <img  alt="" src="{{ section.settings.instaBox_img5 | img_url: '501x' }}">
</a>
        </div>
</div>
</div>


<div class="instagram_section_wrapper mobile_version">
